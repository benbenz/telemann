    "version": "0.2.0",
    "configurations": [
        {
            "name": "django:server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "justMyCode": false,
            "env": {
            },
            "django": true,
            "host": "localhost"
        },
        {
            "name": "django:initsources",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/manage.py",
            "args": [
                "initsources",
            ],
            "justMyCode": false,
            "env": {
            },
            "django": true,
            "host": "localhost"
        },        
    ]
}